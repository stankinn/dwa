<Window x:Class="WpfApp2019.AppServices.Dialog.DialogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2019.AppServices.Dialog"
        xmlns:viewModel="clr-namespace:WpfApp2019.ViewModel"
        xmlns:view="clr-namespace:WpfApp2019.View"
        mc:Ignorable="d"
        Title="{Binding Title}" Height="200" Width="300"
        WindowStartupLocation="CenterScreen"
        SizeToContent="WidthAndHeight"
        >
    <Grid>
        
        <view:DBDialogView/>
    </Grid>
    
   
</Window>
